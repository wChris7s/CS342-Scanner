int foo(int x, int y) {
    if (x > 10 && y == 10) {
        int a = x + y;
        for (int i = 0; i < 5; i = i + 1) {
            a = a + i;
        }
        return a
    } else {
        if (x > y) {
            return x * y;
        }
        return 1;
    }
}

int main(){
    int a = foo (2, 3);
    while (a < 100) {
        a = a / (1 + 2) ;
    }
    return a;
}